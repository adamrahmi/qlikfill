{
  "manifest_version": 3,
  "name": "QlikFill",
  "version": "1.0",
  "description": "Chrome extension to fill text boxes with predefined strings on salesforce.com",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.salesforce.com/*"],
      "js": ["content.js"]
    }
  ],
  // "background": {
  //   "service_worker": "background.js"
  
  // }
}
